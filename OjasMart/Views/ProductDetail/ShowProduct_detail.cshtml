<script>
    function openFileInput() {
        document.getElementById('imgupld').click();
    }

    function displaySelectedImages(event) {
        const files = event.target.files;
        const selectedImagesContainer = document.getElementById('selectedImages');
        const chooseFileOption = document.getElementById('chooseFileOption');

        for (let i = 0; i < files.length; i++) {
            const file = files[i];
            const reader = new FileReader();

            reader.onload = function (e) {
                const img = document.createElement('img');
                img.src = e.target.result;
                img.style.width = '50px&nbsp;';
                img.style.height = '50px';
                selectedImagesContainer.appendChild(img);
            }

            reader.readAsDataURL(file);
        }

        // Always show the plus icon
        chooseFileOption.style.display = 'block';
    }
</script>

<script>

    function SaveReview() {
        debugger;
        var ReviewNum = $('#cskRating').val();
        var ReverName = $('#txtcuname').val();
        var fileInput = document.getElementById('imgupld');
        var RevDis = $('#txtreviewdec').val();
        var ProductId = $("#hdProductId").val();
        $('.form-group span').text('');

        // Check if the name is empty
        if (ReverName.trim() === "") {
            $('#txtcuname').closest('.form-group').find('span').text("Please Enter your Name").css('color', 'red');
            $('#txtcuname').focus();
            return;
        }
        if (ReviewNum.trim() === "") {
            $('#cskRating').closest('.form-group').find('span').text("Please Select Review").css('color', 'red');
            $('#cskRating').focus();
            return;
        }
        if (RevDis.trim() === "") {
            $('#txtreviewdec').closest('.form-group').find('span').text("Please Enter Review").css('color', 'red');
            $('#txtreviewdec').focus();
            return;
        }
        var formData = new FormData();
        formData.append('ReiewStatus', ReviewNum);
        formData.append('CustmerName', ReverName);
        formData.append('ReiewDis', RevDis);
        formData.append('ProductId', ProductId);
        formData.append('image', fileInput.files[0]);

        var files = $('#imgupld')[0].files;
        for (var i = 0; i < files.length; i++) {
            formData.append('image', files[i]); // Selected files ko FormData object mein add karein
        }
        $.ajax({
            url: "/Ecommarce/SaveReview",
            type: "POST",
            data: formData,
            contentType: false,
            processData: false,
            success: function (data1) {
                if (data1 != "0") {
                    // Create a message element
                    var successMessageElement = $('<div>').addClass('alertMessage success').text(data1);

                    // Create success icon element
                    var successIcon = $('<i>').addClass('fa fa-check-circle').css({ 'color': 'green', 'margin-right': '5px' });



                    // Append success icon to the message element
                    successMessageElement.prepend(successIcon);

                    // Append the message element to the body
                    $('body').append(successMessageElement);

                    // Set timeout to remove the message after 3 seconds
                    setTimeout(function () {
                        successMessageElement.remove();
                    }, 3000);

                    BindReviewDetails();
                    $('#txtcuname').val('');
                    $('#txtreviewdec').val('');
                    $('#imgupld').val('');
                } else {
                    // Create a message element
                    var errorMessageElement = $('<div>').addClass('alertMessage error').text("Server Error. Please try after some time.");

                    // Create error icon element
                    var errorIcon = $('<i>').addClass('fa fa-exclamation-circle').css({ 'color': 'red', 'margin-right': '5px' });

                    // Append error icon to the message element
                    errorMessageElement.prepend(errorIcon);

                    // Append the message element to the body
                    $('body').append(errorMessageElement);

                    // Set timeout to remove the message after 3 seconds
                    setTimeout(function () {
                        errorMessageElement.remove();
                    }, 3000);
                }
            }
        });

    }


    function BindReviewDetails() {
        $.ajax({
            url: "/SEOURL/SaleProduct",
            type: "POST",
            data: { 'Category': $.trim($('#hdCategory').val()), 'ProductName': $.trim($('#hdproductTitle').val()), 'ProductId': $.trim($('#hdproductId').val()) },
            datatype: "json",
            success: function (data1) {
                debugger;
                var htmlNew = data1;
                $('#review').html($(htmlNew).find('#tblReview'));
                // $('#divtable').html($(htmlNew).find('#divcnt'));
            }
        });
    }

</script>
