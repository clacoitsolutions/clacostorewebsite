    function UpdateTableQty(obj, btntype) {
        debugger;
        var NetTot = 0, RegAmt = 0;
        var qty = 0;
        qty = $(obj).parent().find('input[type=number]').val() != '' ? parseInt($(obj).parent().find('input[type=number]').val()) : 1;
        if (btntype == 'A') {
            $(obj).parent().find('input[type=number]').val(qty + 1);
        }
        else {
            if (qty == 1) {
                return false;
            }
            $(obj).parent().find('input[type=number]').val(qty - 1);
        }

        $('#divItemSummary .border_radius').each(function () {
            var row = $(this);
            item = {};
            var UnitRate = row.attr('UnitRate') != '' ? parseFloat(row.attr('UnitRate')) : 0;
            var RegRate = row.attr('RegRate') != '' ? parseFloat(row.attr('RegRate')) : 0;
            qty = row.find('input[type=number]').val() != '' ? parseInt(row.find('input[type=number]').val()) : 1;
            var TotAmt = UnitRate * (qty);
            var RegTot = RegRate * (qty);
            NetTot += TotAmt;
            RegAmt += RegTot;
        });

        var totSaving = (parseFloat(RegAmt) - parseFloat(NetTot));
        $('#SpntotSaving').text(totSaving.toFixed(2));
        $('#spnTotalAmount').text(NetTot.toFixed(2));
        $('#hdNetAmt').val(NetTot.toFixed(2));
    }

