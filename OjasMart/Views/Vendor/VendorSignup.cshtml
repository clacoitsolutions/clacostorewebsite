    <script type="text/javascript">
        function allowOnlyDigit(evt, obj) {
            var charCode = (evt.which) ? evt.which : event.keyCode
            if (charCode == 45) {
                var val = obj.value;
                if (val.length > 0)
                    return false;
            }
            else if (charCode > 31 && (charCode < 48) || (charCode > 57)) {
                return false;
            }
            return true;
        }
    </script>

