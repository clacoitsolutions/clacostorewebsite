        top: 0px;
        background: none !important;
    }

    .like-icon::before {
        content: "";
        transform: scale(0.95);
        background: none !important;
    }

    .like-icon::after {
        content: "";
        opacity: 0;
        transform: scale(0.3);
        font-weight: 900;
    }

    .like-icon.liked::after {
        opacity: 1;
        transform: scale(0.95);
        background: none !important;
    }

    .like-icon:hover, .like-icon.liked {
        /*background-color: rgb(237 26 99);*/
        color: #fa2365;
        background: none;
    }

        .like-icon.liked::before {
