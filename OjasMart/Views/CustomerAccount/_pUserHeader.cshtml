@model OjasMart.Models.PropertyClass
    @{ 
        Layout = null;
    }

<div class="user-dt">
    <div class="user-img">
        <img src="/webassets/images/avatar/img-5.jpg" alt="">
        <div class="img-add" style="display:none">
            <input type="file" id="file">
            <label for="file"><i class="uil uil-camera-plus"></i></label>
        </div>
    </div>
    <h4>@Model.SSName</h4>
    <p>+91-@Model.ContactNo<a href="#" style="display:none"><i class="uil uil-edit"></i></a></p>
    <div class="earn-points" style="display:none"><img src="/webassets/images/Dollar.svg" alt="">Points : <span>20</span></div>
</div>
