    function ApplyPromoCode() {
        debugger;
        if ($('#txtPromoCode').val() == '') {
            alert('Please Enter Promocode !');
            $('#txtPromoCode').focus();
            return;
        }
        var dataobject = {
            PromoCode: $('#txtPromoCode').val(),

            Orderamount: (@Model.PaidAmount-@Model.deliverycharges),
            //Orderamount: $('#spnTotalAmount').val(),


        };
        $.ajax({
            type: "POST",
            url: "/Checkout/GetPromoCode",
            data: dataobject,
            dataType: "json",
            success: function (r) {
                if (r.RespoCode == "1") {
                    var TotalAmt = (@Model.PaidAmount-@Model.deliverycharges) != '' ? parseFloat(@Model.PaidAmount-@Model.deliverycharges) : 0;
                    var deiverycharger = 0;
                    deliverycharges = @Model.deliverycharges != '' ? parseFloat(@Model.deliverycharges) : 0;
                    var PromoAmt = 0;
                    if (r.OfferType == "Amount") {
                        PromoAmt = r.DiscPer;
                        $('#spnInfo').text("Rs." + r.DiscPer + " off.");
                    }
                    else {
                        PromoAmt = (TotalAmt * r.DiscPer) / 100;
                        $('#spnInfo').text("" + r.DiscPer + "% off.");
                    }
                    TotalAmt = TotalAmt - PromoAmt;

                    TotalAmt = TotalAmt + deliverycharges;
                    $("#divPromo").hide();
                    $(".promo-link45").hide();
                    $('#spnPromo').text(Math.round(PromoAmt.toFixed(2)));
                    $('#spnTotalAmount').text(Math.round(TotalAmt.toFixed(2)));
                    $('#divApplyPromo').show();
                }
                else {
                    alert('Invalid Promocode.');
                    var TotalAmt =@Model.PaidAmount != '' ? parseFloat(@Model.PaidAmount) : 0;
                    $('#spnTotalAmount').text(Math.round(@Model.PaidAmount));
                    $('#spnInfo').text('');
                    $('#divApplyPromo').hide();
                }
            }
        });

    }

    function CancelPromo() {
        debugger;
        var TotalAmt = $('#hdNetAmt').val() != '' ? parseFloat($('#hdNetAmt').val()) : 0;
        $('#spnTotalAmount').text(Math.round(TotalAmt.toFixed(2)));
        $('#spnInfo').text('');
        $('#divApplyPromo').hide();
        $("#divPromo").show();
        $(".promo-link45").show();
    }
</script>
