    <script>
        $(document).ready(function () {
           toastr.options = {
            "closeButton": true,
            "debug": false,
            "positionClass": "toast-top-right",
            "onclick": null,
            "showDuration": "1000",
            "hideDuration": "1000",
            "timeOut": "5000",
            "extendedTimeOut": "1000",
            "showEasing": "swing",
            "hideEasing": "linear",
            "showMethod": "fadeIn",
            "hideMethod": "fadeOut"
           };



           $(".allownumericwithdecimal").on("keypress keyup blur", function (event) {
               //this.value = this.value.replace(/[^0-9\.]/g,'');
               $(this).val($(this).val().replace(/[^0-9\.]/g, ''));
               if ((event.which != 46 || $(this).val().indexOf('.') != -1) && (event.which < 48 || event.which > 57)) {
                   event.preventDefault();
               }
           });
        });
    </script>

    <style>
        .mCSB_outside {
            background: black;
        }

        .header-navbar .navbar-wrapper {
            background: black !important;
        }

        .main-menu-header {
            background: black !important;
        }




        .footer {
            background-color: #F7F8FA;
            position: fixed;
            z-index: 99999;
        }

            .footer .copyright {
                background: #ca0008;
                background: linear-gradient(black, black);
            }

                .footer .copyright p {
                    text-align: center;
                    margin: 0;
                    padding: 0px;
                    color: white;
                }
    </style>


</head>
<body>
    <!-- Begin page -->
    <div id="wrapper">
        <!-- Topbar Start -->
        <div class="navbar-custom">
            <ul class="list-unstyled topnav-menu float-right mb-0">

                @if (Convert.ToString(Session["Role"]) == "4")
                {
                    <li class="dropdown notification-list">
                        <a class="nav-link dropdown-toggle  waves-effect waves-light" href="#" role="button" aria-haspopup="false" aria-expanded="false">
                            <i class="fas fa-wallet"></i> Wallet Balance: <span>@Session["WalletBal"]</span>
                        </a>
                    </li>
                    @Html.Action("_pBindCartList", "Ecommarce")
                }

                <li class="dropdown notification-list">
                    <a class="nav-link dropdown-toggle nav-user mr-0 waves-effect waves-light" data-toggle="dropdown" href="#" role="button" aria-haspopup="false" aria-expanded="false">
                        <img src="../assets/images/users/UserIco.png" alt="user-image" class="rounded-circle" style="display:none;">
                        <span class="pro-user-name ml-1">
                            @Session["CompanyName"] <i class="mdi mdi-chevron-down"></i>
                        </span>
                    </a>
                    <div class="dropdown-menu dropdown-menu-right profile-dropdown ">
                        <!-- item-->
                        <div class="dropdown-header noti-title">
                            <h6 class="text-overflow m-0">Welcome !</h6>
                        </div>
                        <!-- item-->
                        @if (Convert.ToString(Session["Role"]) != "1" && Convert.ToString(Session["Role"]) != "4")
                        {
                        <a href="../SuperStockiest/MyProfile" class="dropdown-item notify-item">
                            <i class="fe-user"></i>
                            <span>My Account</span>
                        </a>
                        }
                        else if (Convert.ToString(Session["Role"]) == "4")
                        {
                        <a href="../Home/CustomerProfile" class="dropdown-item notify-item">
                            <i class="fe-user"></i>
                            <span>My Account</span>
                        </a>
                        }
                        else if (Convert.ToString(Session["Role"]) == "10")
                        {
                            <a href="../Vendor/SellerProfile" class="dropdown-item notify-item">
                                <i class="fe-user"></i>
                                <span>My Account</span>
                            </a>
                        }

                        <a href="../CustomerAccount/ChangePassword" class="dropdown-item notify-item">
                            <i class="fe-lock"></i>
                            <span>Change  Password</span>
                        </a>

                        <!-- item-->
                        @*<a href="javascript:void(0);" class="dropdown-item notify-item">
                                <i class="fe-settings"></i>
                                <span>Settings</span>
                            </a>
                            <a href="javascript:void(0);" class="dropdown-item notify-item">
                                <i class="fe-lock"></i>
                                <span>Lock Screen</span>
                            </a>*@
                        <div class="dropdown-divider"></div>
                        <!-- item-->
                        <a href="@Url.Action("Logout","Account")" class="dropdown-item notify-item">
                            <i class="fe-log-out"></i>
                            <span>Logout</span>
                        </a>
                    </div>
                </li>

            </ul>
            <!-- LOGO -->
            <div class="logo-box">
                <a href="../Home/Dashboard" class="logo text-center">
                    <span class="logo-lg">
                        <img src="../img/logo.png" alt="" style="height:55px">
                    </span>
                    <span class="logo-sm">
                        <span class="logo-sm-text-dark" style="color:#fff">Claco Store</span>
                    </span>
                </a>
            </div>
            <ul class="list-unstyled topnav-menu topnav-menu-left m-0">
                <li>
                    <button class="button-menu-mobile waves-effect waves-light">
                        <i class="fe-menu"></i>
                    </button>
                </li>
                

                <li class="dropdown d-none d-lg-block">
                    <a class="nav-link dropdown-toggle  waves-effect waves-light" href="#" role="button" aria-haspopup="false" aria-expanded="false">
                        <i class="fa fa-calendar"></i> Last login at: <span>@Session["LastLogedIn"]</span>
                    </a>
                </li>

            </ul>
        </div>
        <!-- end Topbar -->
        <!-- ========== Left Sidebar Start ========== -->
        <div class="left-side-menu">
            <div class="slimscroll-menu">
                <!--- Sidemenu -->
                <div id="sidebar-menu">
                    @if (Session["Role"] != null && Session["Role"].ToString() != "")
            {
                { Html.RenderAction("GetMenuList", "Home"); }
            }
            else
            {
                var redirectUrl = Url.Action("Index", "Account");
                        <script>
                            var redirectUrl = '@redirectUrl';
                            window.location.href = redirectUrl;
                        </script>
                    }


                </div>
                <!-- End Sidebar -->
                <div class="clearfix"></div>
            </div>
            <!-- Sidebar -left -->
        </div>
        <!-- Left Sidebar End -->
        <!-- ============================================================== -->
        <!-- Start Page Content here -->
        <!-- ============================================================== -->
        <div class="content-page">
            <div class="content">
                <!-- Start Content-->
                <div class="container-fluid">

                    @RenderBody()

                </div> <!-- container -->
            </div> <!-- content -->
            <!-- Footer Start -->



