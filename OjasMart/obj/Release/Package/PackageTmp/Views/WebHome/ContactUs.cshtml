@model OjasMart.Models.ClsContactUs
@{
    ViewBag.Title = "ContactUs";
    Layout = "~/Views/Shared/_FrontLayout.cshtml";
}

<script src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.1/jquery.min.js"></script>
<script>
    $(document).ready(function () {
        if ('@TempData["msg"]'!='') {
            alert('@TempData["msg"]');
        }
    });
</script>

<style>
    /*.wrapper {
    padding-top: 131px;
    padding-bottom: 50px;
}*/

</style>
    <div class="gambo-Breadcrumb">
        <div class="container">
            <div class="row">
                <div class="col-md-12">
                    <nav aria-label="breadcrumb">
                        <ol class="breadcrumb">
                            <li class="breadcrumb-item"><a href="index.html">Home</a></li>
                            <li class="breadcrumb-item active" aria-current="page">Contact Us</li>
                        </ol>
                    </nav>
                </div>
            </div>
        </div>
    </div>
@using (Html.BeginForm())
{
    <div class="all-product-grid">
        <div class="container">
            <div class="row">
                <div class="col-lg-6 col-md-6">
                    <div class="panel-group " id="">
                        <div class="panel panel-default">
                            <div class="panel-heading" id="headingOne">
                                <div class="panel-title">
                                    <a class="collapsed" data-toggle="collapse" data-target="#collapseOne" href="#" aria-expanded="false" aria-controls="collapseOne">
                                        <i class="uil uil-location-point chck_icon"></i>HEAD OFFICE ADDRESS
                                    </a>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="panel-collapse">
                        <div class="panel-body">
                            <h2>Claco Store</h2>
                            <h3>
                                Indrarekha Developers
                            </h3>
                            <h3 style="font-weight: 200; color: black;">
                                11A/28, Vrindavan Colony SEC -11 A, District: Lucknow, UP -226010
                            </h3>
                            <h3 style="font-weight: 200; color: black;">
                                +91-7275949944
                            </h3>
                        </div>
                    </div>
                </div>
                
                <div class="col-lg-6 col-md-6">
                    <div class="contact-title">
                        <h2>Submit customer service request</h2>
                        <p>If you have a question about our service or have an issue to report, please send a request and we will get back to you as soon as possible.</p>
                    </div>
                    <div class="contact-form">
                        <form>
                            <div class="form-group mt-1">
                                <label class="control-label">Full Name*</label>
                                <div class="ui search focus">
                                    <div class="ui left icon input swdh11 swdh19">
                                        @Html.TextBoxFor(model => model.FullName, new { @id = "txtFullName", @placeholder = "Enter Full Name...", @class = "prompt srch_explore", @Required = true })
                                        @*<input class="prompt srch_explore" type="text" name="sendername" id="sendername" required="" placeholder="Your Full">*@
                                    </div>
                                </div>
                            </div>
                            <div class="form-group mt-1">
                                <label class="control-label">Mobile No*</label>
                                <div class="ui search focus">
                                    <div class="ui left icon input swdh11 swdh19">
                                        @*<input class="prompt srch_explore" type="text" name="sendersubject" id="sendersubject" required="" placeholder="Subject">*@
                                        @Html.TextBoxFor(model => model.mobileNo, new { @id = "txtmobileNo", @placeholder = "Enter mobileNo ...", @class = "prompt srch_explore", @Required = true })
                                    </div>
                                </div>
                            </div>
                            <div class="form-group mt-1">
                                <label class="control-label">Email Address*</label>
                                <div class="ui search focus">
                                    <div class="ui left icon input swdh11 swdh19">
                                        @Html.TextBoxFor(model => model.EmailAddress, new { @id = "txtemailaddress", @placeholder = "Enter Email Address...", @class = "prompt srch_explore", @Required = true })
                                        @*<input class="prompt srch_explore" type="email" name="emailaddress" id="emailaddress" required="" placeholder="Your Email Address">*@
                                    </div>
                                </div>
                            </div>
                          
                            <div class="form-group mt-1">
                                <div class="field">
                                    <label class="control-label">Message</label>
                                    @Html.TextAreaFor(model => model.Message, new { @rows="2", @id = "sendermessage", @placeholder = "Write Message ...", @class="form-control" })
                                    @*<textarea rows="2" class="form-control" id="sendermessage" name="sendermessage" required="" placeholder="Write Message"></textarea>*@
                                </div>
                            </div>
                            <button class="next-btn16 hover-btn mt-3" type="submit" data-btntext-sending="Sending...">Submit Request</button>
                        </form>
                    </div>
                </div>
        
            </div>
        </div>
    </div>
}

