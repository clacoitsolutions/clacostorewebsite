<script>
    window.addEventListener('scroll', function () {
        var phnDiv = document.querySelector('.phn');
        var ddphnDiv = document.querySelector('.p');

        var phnHeight = phnDiv.offsetHeight;
        var ddphnOffsetTop = ddphnDiv.offsetTop;

        if (window.pageYOffset >= ddphnOffsetTop - phnHeight) {
            phnDiv.style.position = 'absolute';
            phnDiv.style.bottom = 'initial';
            phnDiv.style.top = ddphnOffsetTop - phnHeight + 'px';
        } else {
            phnDiv.style.position = 'fixed';
            phnDiv.style.bottom = '0';
            phnDiv.style.top = 'initial';
        }
    });
</script>

