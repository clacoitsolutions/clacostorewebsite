            <script>
                function Val()
                {
           
                    if ($("#CustomerName").val()=="") {
                        alert("Please Enter Member Name..");
                        $("#CustomerName").focus();
                        return;
                    }
                    if ($("#MobileNo").val() == "") {
                        alert("Please Enter Mobile No..");
                        $("#MobileNo").focus();
                        return;
                    }
                    if ($("#EmailAddress").val() == "") {
                        alert("Please Enter Email Address..");
                        $("#EmailAddress").focus();
                        return;
                    }
                    if ($("#Password").val() == "") {
                        alert("Please Enter Password..");
                        $("#Password").focus();
                        return;
                    }
                    if ($("#ddlMemberShipType").val() == "") {
                        alert("Please Select MemberShip Type..");
                        $("#ddlMemberShipType").focus();
                        return;
                    }
                    $("#btnSubmit").attr('type', 'submit');
                }
            </script>

