                                    function shareViaWhatsApp() {
                                        var productPath = "/path/to/product"; // Replace with your actual product path
                                        var shareText = "Check out this product: " + window.location.origin + productPath;
                                        var encodedShareText = encodeURIComponent(shareText);
                                        window.open("whatsapp://send?text=" + encodedShareText);
                                    }


                                    function shareViaInstagram() {
                                        var productPath = "/path/to/product"; // Replace with your actual product path
                                        var shareUrl = encodeURIComponent(window.location.origin + productPath);
                                        window.open("https://www.instagram.com/share?url=" + shareUrl);
                                    }

                                    function shareViaEmail() {
                                        var productPath = "/path/to/product"; // Replace with your actual product path
                                        var subject = encodeURIComponent("Check out this product");
                                        var body = encodeURIComponent("I thought you might find this interesting: " + window.location.origin + productPath);
                                        window.location.href = "mailto:?subject=" + subject + "&body=" + body;
                                    }
                                    function copyProductLink() {
                                        debugger;
                                        var currentURL = window.location.href;
                                        var productPath = currentURL; // Set productPath to current URL
                                        var productLink = window.location.origin + productPath;

                                        var tempInput = document.createElement("input");
                                        tempInput.value = productLink;
                                        document.body.appendChild(tempInput);
                                        tempInput.select();
                                        document.execCommand("copy");
                                        document.body.removeChild(tempInput);

                                        alert("Product link copied to clipboard!");
                                    }
