@model OjasMart.Models.ClsMemberShip
@{
    ViewBag.Title = "PrintmemberCard";
    Layout = null;
}



<!DOCTYPE html>
<html lang="en">
<meta http-equiv="content-type" content="text/html;charset=UTF-8" />
<head>
    <meta charset="utf-8">

    <meta name="viewport" content="width=device-width, initial-scale=1.0" />

    <link href="https://fonts.googleapis.com/css2?family=Rajdhani:wght@300;400;500;600;700&amp;display=swap" rel="stylesheet">
    <link href='http://tikbasket.co.in/webassets/vendor/unicons-2.0.1/css/unicons.css' rel='stylesheet'>
    <link href="http://tikbasket.co.in/webassets/css/style.css" rel="stylesheet">
    <link href="http://tikbasket.co.in/webassets/css/responsive.css" rel="stylesheet">
    <link href="http://tikbasket.co.in/webassets/css/night-mode.css" rel="stylesheet">
    <link href="http://tikbasket.co.in/webassets/css/step-wizard.css" rel="stylesheet">

    <!-- Vendor Stylesheets -->
    <link href="http://tikbasket.co.in/webassets/vendor/fontawesome-free/css/all.min.css" rel="stylesheet">
    <link href="http://tikbasket.co.in/webassets/vendor/OwlCarousel/assets/owl.carousel.css" rel="stylesheet">
    <link href="http://tikbasket.co.in/webassets/vendor/OwlCarousel/assets/owl.theme.default.min.css" rel="stylesheet">
    <link href="http://tikbasket.co.in/webassets/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">
    <link rel="stylesheet" type="text/css" href="http://tikbasket.co.in/webassets/vendor/semantic/semantic.min.css">


    <style>

        .lff{float:left}
        .rtt{float:right;    margin: 0 26px 0 1px;}
.ww,print{    background: #005aa3;
    color: white;
    font-weight: 300;
    padding: 11px 16px;}

.q1,print{     width: 69%;
    float: left;
    padding: 20px;}
.q2,print{    width: 22%;    float: left;}
.qw12,print{     width: 640px;
    display: flow-root;
    background: #d9dcdf;
    border-radius: 12px;
    overflow: hidden;
    height: 340px;}
.gh{    margin: 10px 0 0;}

@@media print{
.ww,print{    background: #005aa3 !important;
    color: white;
    font-weight: 300;
    padding: 11px 16px;}

.q1,print{     width: 69%;
    float: left;
    padding: 20px;}
.q2,print{    width: 22%;    float: left;}
.qw12,print{     width: 640px;
    display: flow-root;
    background: #d9dcdf !important;
    border-radius: 12px;
    overflow: hidden;
    height: 340px;}
}




    </style>
</head>
<body class="  ">

    <div class="qw12">

        <h2 class="ww">@Model.CardId</h2>

        <div class="qw122">
            <div class="q1">
                <div classs="icard_text">

                    <h2>@Model.CustomerName</h2>
                    <p>@Model.CompanyName</p>

                    <h2>@Model.CardType</h2>
                    <div class="datetime">
                        <span class="lff">Since @Model.CardValidFrom
                    </div>
                    <span class="rtt">Valid Thru @Model.CardValidTo
                </div>
                
            </div>



            <div class="q2">
                <img src="@Model.ProfilePicPath" style="width:130Px;height:130Px" />
                <div class="text-center gh">
                <img src="../img/logo.png" style="max-width: 100%;height: 70px;" />
                </div>
            </div>
        </div>
    </div>
    </div>



    <script src="http://tikbasket.co.in/webassets/js/jquery-3.3.1.min.js"></script>
    <script src="http://tikbasket.co.in/webassets/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="http://tikbasket.co.in/webassets/vendor/OwlCarousel/owl.carousel.js"></script>
    <script src="http://tikbasket.co.in/webassets/vendor/semantic/semantic.min.js"></script>
    <script src="http://tikbasket.co.in/webassets/js/jquery.countdown.min.js"></script>
    <script src="http://tikbasket.co.in/webassets/js/custom.js"></script>
    <script src="http://tikbasket.co.in/webassets/js/product.thumbnail.slider.js"></script>
    <script src="http://tikbasket.co.in/webassets/js/offset_overlay.js"></script>
    <script src="http://tikbasket.co.in/webassets/js/night-mode.js"></script>
    <script src="http://tikbasket.co.in/webassets/js/notify.js"></script>

</body>

</html>
