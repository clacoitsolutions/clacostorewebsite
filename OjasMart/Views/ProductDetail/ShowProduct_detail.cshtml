    function AddToWhishlist() {
        /* start codeby muskan 6may */
        var data = new FormData();
        var Id = $("#hdProductId").val();
        data.append("ProductId", Id);

        $.ajax({
            url: "../ProductDetail/AddToWhishlist",
            type: "POST",
            contentType: false,
            processData: false,
            data: data,
            success: function (r) {
                // Create a message element
                var messageElement = $('<div>').addClass('alertMessage').text(r.msg);

                // Append it to the body
                $('body').append(messageElement);

                // Set timeout to remove the message after 3 seconds
                setTimeout(function () {
                    messageElement.remove();
                }, 3000);
            },
            error: function (XMLHttpRequest, textStatus, errorThrown) {
                // Handle error
                $("#showSpinner").hide();
            }
        });
    }

