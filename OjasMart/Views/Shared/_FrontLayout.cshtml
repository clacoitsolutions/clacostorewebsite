    <script>
        document.addEventListener("DOMContentLoaded", function () {
            var content = document.getElementById("content");
            var dot3btn = document.querySelector(".dot3btn");

            // Toggle content visibility on click
            function toggleContent() {
                content.classList.toggle("show");
            }

            // Attach click event listener to dot3btn
            dot3btn.addEventListener("click", function (event) {
                toggleContent();
                event.stopPropagation(); // Prevents the click event from propagating to the document
            });

            // Close content when clicking outside of it
            document.addEventListener("click", function (event) {
                if (!content.contains(event.target) && event.target !== dot3btn) {
                    content.classList.remove("show");
                }
            });

            // Close content when clicking the content itself
            content.addEventListener("click", function (event) {
                event.stopPropagation();
            });
        });
    </script>
    <script>
        document.addEventListener("DOMContentLoaded", function () {
            var searchInput = document.getElementById("txtsearchmain");
            var categoryDialog = document.getElementById("category-dialog");

            searchInput.addEventListener("click", function (event) {
                categoryDialog.style.display = "block";
                event.stopPropagation(); // Prevents the click event from propagating to the document
            });

            categoryDialog.addEventListener("click", function (event) {
                event.stopPropagation(); // Prevents the click event from propagating to the document
            });

            document.addEventListener("click", function () {
                categoryDialog.style.display = "none";
            });
        });

        function setSearchValue(categoryName) {
            document.getElementById("txtsearchmain").value = categoryName;
        }

        function searchByCategory(categoryName) {
            // Perform any actions related to searching by category
            console.log("Search by category: " + categoryName);
        }
    </script>

