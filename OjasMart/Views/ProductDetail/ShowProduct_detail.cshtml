        var star5 = $(".star5").text();
        var star_5 = parseInt(star5);

        var totalStars = star_1 + star_2 + star_3 + star_4 + star_5;
        var averageRating = (star_1 * 1 + star_2 * 2 + star_3 * 3 + star_4 * 4 + star_5 * 5) / totalStars;

        $(".bar-1").css("width", star_1 > 0 ? (star_1 / totalStars) * 100 + "%" : "0%");
