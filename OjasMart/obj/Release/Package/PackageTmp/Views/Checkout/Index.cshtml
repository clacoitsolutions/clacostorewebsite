    function RemoveItemFromCart(pId) {
        var mm = confirm('are you sure to remove this Item?');
        if (mm) {
            if (pId == '') {
                return;
            }
            var dataobject = {
                ProductId: pId,
                IPAddress: null
            };
            $("#showSpinner").show();
            $.ajax({
                url: "@Url.Action("RemoveFromCart", "AddToCart")",
                type: "POST",
                data: dataobject,
                datatype: "json",
                success: function (data) {
                    if (data == "1") {
                        location.reload();
                        $("#showSpinner").hide();
                    }
                    else {
                        alert('Something went wrong!')
                        $("#showSpinner").hide();
                    }
                },
                error: function () {
                    alert('Error!')
                    $("#showSpinner").hide();
                    //cart.add('69');
                }
            });
        }
        return;
    }

