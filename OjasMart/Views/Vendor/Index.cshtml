<script>
    document.addEventListener("DOMContentLoaded", function () {
        // Get all the panel titles
        var panelTitles = document.querySelectorAll('.panel-title');

        // Loop through each panel title
        panelTitles.forEach(function (panelTitle) {
            // Add click event listener to each panel title
            panelTitle.addEventListener('click', function (event) {
                // Prevent the default click behavior, which includes scrolling
                event.preventDefault();

                // Toggle the 'collapsed' class on the panel title
                this.classList.toggle('collapsed');

                // Find the corresponding panel body
                var panelBody = this.closest('.card').querySelector('.panel-collapse');

                // Close all other panels
                var allPanels = document.querySelectorAll('.panel-collapse');
                allPanels.forEach(function (panel) {
                    if (panel !== panelBody) {
                        panel.classList.remove('show');
                        panel.previousElementSibling.querySelector('.panel-title').classList.add('collapsed');
                    }
                });

                // Toggle the 'show' class on the panel body
                panelBody.classList.toggle('show');
            });
        });
    });







</script>

