<script>
    // Function to hide the <h1> element
    function hideHeading() {
        document.querySelector('#create-account8 h1').style.display = 'none';
        document.querySelector('#create-account11 h1').style.display = 'none';
        document.querySelector('#create-account10 h1').style.display = 'none';
        document.querySelector('#create-account3 h1').style.display = 'none';
        document.querySelector('#create-account4 h1').style.display = 'none';
        document.querySelector('#create-account5 h1').style.display = 'none';
    }

    // Function to show the <h1> element
    function showHeading() {
        document.querySelector('#create-account8 h1').style.display = 'block';
        document.querySelector('#create-account11 h1').style.display = 'block';
        document.querySelector('#create-account10 h1').style.display = 'block';
        document.querySelector('#create-account3 h1').style.display = 'block';
        document.querySelector('#create-account4 h1').style.display = 'block';
        document.querySelector('#create-account5 h1').style.display = 'block';
    }

    // Function to update the active state based on scroll position
    function updateActiveState() {
        // Get all sections
        const sections = document.querySelectorAll('.row > div[id]');

        // Get current scroll position
        const scrollPosition = window.scrollY;

        // Loop through each section
        sections.forEach(section => {
            const top = section.offsetTop; // Top position of the section
            const bottom = top + section.offsetHeight;

            // Check if the current scroll position is within the section with a 100px margin from the top
            if (scrollPosition >= (top - 100) && scrollPosition < bottom) {
                // Remove active class from all list items
                document.querySelectorAll('.item-list').forEach(item => {
                    item.classList.remove('active');
                    item.classList.remove('Border-design'); // Remove Border-design from all items
                });

                // Add active class and Border-design to the corresponding list item
                const itemId = section.getAttribute('id');
                const correspondingListItem = document.querySelector(`a[href="#${itemId}"]`);
                if (correspondingListItem) {
                    correspondingListItem.querySelector('.item-list').classList.add('active');
                    if (itemId.startsWith('create-account8')) { // Apply Border-design to all create-account sections
                        correspondingListItem.querySelector('.item-list').classList.add('Border-design');
                    }
                }
            }
        });
    }

    // Add event listener for scroll event
    window.addEventListener('scroll', updateActiveState);

    // Call the function initially to set the correct active state
    updateActiveState();

    // Add click event listeners to navigation buttons
    document.querySelectorAll('.item-list').forEach(item => {
        item.addEventListener('click', function (event) {
            event.preventDefault(); // Prevent default link behavior
            const sectionId = this.getAttribute('href').substr(1); // Get section ID from href attribute
            scrollToSection(sectionId);
        });
    });

    // Function to handle smooth scrolling to section with a margin from the top
    // Add click event listener to the "Create Account" button to show the <h1> element and scroll to the section
    document.addEventListener('DOMContentLoaded', function () {
        hideHeading(); // Hide the <h1> element initially

        // Function to handle smooth scrolling to section with a margin from the top
        function scrollToSection(sectionId) {
            debugger;
            const section = document.getElementById(sectionId);
            if (section) {
                const topOffset = section.offsetTop - 100; // Adjusted top position with margin
                window.scrollTo({
                    top: topOffset,
                    behavior: 'smooth'
                });

                // Remove active class and Border-design from all list items
                document.querySelectorAll('.item-list').forEach(item => {
                    item.classList.remove('active');
                    item.classList.remove('Border-design');
                });

                // Add active class and Border-design to the clicked list item
                const correspondingListItem = document.querySelector(`a[href="#${sectionId}"]`);
                if (correspondingListItem) {
                    correspondingListItem.querySelector('.item-list').classList.add('active');
                    if (sectionId.startsWith('create-account8')) { // Apply Border-design to all create-account sections
                        correspondingListItem.querySelector('.item-list').classList.add('Border-design');
                    }
                }
            }
        }

        document.querySelector('#create-account-button8').addEventListener('click', function () {
            showHeading(); // Show the <h1> element when the button is clicked
            scrollToSection('create-account8'); // Scroll to the corresponding section
        });

        document.querySelector('#create-account-button11').addEventListener('click', function () {
            debugger;
            showHeading(); // Show the <h1> element when the button is clicked
            scrollToSection('create-account11'); // Scroll to the corresponding section
        });

        document.querySelector('#create-account-button10').addEventListener('click', function () {
            showHeading(); // Show the <h1> element when the button is clicked
            scrollToSection('create-account10'); // Scroll to the corresponding section
        });
        document.querySelector('#create-account-button3').addEventListener('click', function () {
            debugger;
            showHeading(); // Show the <h1> element when the button is clicked
            scrollToSection('create-account3'); // Scroll to the corresponding section
        });
        document.querySelector('#create-account-button4').addEventListener('click', function () {
            showHeading(); // Show the <h1> element when the button is clicked
            scrollToSection('create-account4'); // Scroll to the corresponding section
        });
        document.querySelector('#create-account-button5').addEventListener('click', function () {
            showHeading(); // Show the <h1> element when the button is clicked
            scrollToSection('create-account5'); // Scroll to the corresponding section
        });


        // Add event listener for scroll event
        window.addEventListener('scroll', updateActiveState);

        // Call the function initially to set the correct active state
        updateActiveState();
    });


    // Add click event listener to the "Create Account" button to show the <h1> element and scroll to the section
    document.addEventListener('DOMContentLoaded', function () {
        hideHeading(); // Hide the <h1> element initially
        document.querySelector('#create-account-button8').addEventListener('click', function () {
            showHeading(); // Show the <h1> element when the button is clicked
            scrollToSection('create-account8'); // Scroll to the corresponding section
        });
        document.querySelector('#create-account-button11').addEventListener('click', function () {
            showHeading(); // Show the <h1> element when the button is clicked
            scrollToSection('create-account11'); // Scroll to the corresponding section
        });
        document.querySelector('#create-account-button10').addEventListener('click', function () {
            showHeading(); // Show the <h1> element when the button is clicked
            scrollToSection('create-account10'); // Scroll to the corresponding section
        });
        document.querySelector('#create-account-button3').addEventListener('click', function () {
