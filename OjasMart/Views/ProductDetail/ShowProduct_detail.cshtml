        $(".bar-3").css("width", star_3 > 0 ? (star_3 / totalStars) * 100 + "%" : "0%");
        $(".bar-4").css("width", star_4 > 0 ? (star_4 / totalStars) * 100 + "%" : "0%");
        $(".bar-5").css("width", star_5 > 0 ? (star_5 / totalStars) * 100 + "%" : "0%");

        $(".averageRating").text(averageRating > 0 ? averageRating.toFixed(1) : 0);
