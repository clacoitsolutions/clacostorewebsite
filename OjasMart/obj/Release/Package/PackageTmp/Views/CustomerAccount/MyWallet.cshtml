@{
    ViewBag.Title = "MyWallet";
    Layout = "~/Views/Shared/_FrontLayout.cshtml";
}
@using System.Data;
<div class="gambo-Breadcrumb">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <nav aria-label="breadcrumb">
                    <ol class="breadcrumb">
                        <li class="breadcrumb-item"><a href="../WebHome/Index">Home</a></li>
                        <li class="breadcrumb-item active" aria-current="page">My Wallet</li>
                    </ol>
                </nav>
            </div>
        </div>
    </div>
</div>
<div class="dashboard-group">
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                @Html.Action("_pUserHeader", "CustomerAccount")
            </div>
        </div>
    </div>
</div>
<div class="">
    <div class="container">
        <div class="row">
            @Html.Action("_pUserSideMenu", "CustomerAccount")
            <div class="col-lg-9 col-md-8" id="div">
                <div class="dashboard-right" id="div1">
                    <div class="row">
                        <div class="col-md-12">
                            <div class="main-title-tab">
                                <h4><i class="uil uil-box"></i>My Wallet</h4>
                            </div>
                        </div>
                        <div class="col-lg-12 col-md-12" style="text-align:center">
                            <img src="/img/wallet.png" class="img-fluid" style="height:150px;">
                            <div class="col-lg-12 col-md-12" style="text-align: center; margin-top: 3%;">
                                <h4> <span style="font-weight: 800; color: #542a16; font-size: 50px;" id="spwalletamt"> ₹ @ViewBag.WalletAmount/-</span></h4>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>