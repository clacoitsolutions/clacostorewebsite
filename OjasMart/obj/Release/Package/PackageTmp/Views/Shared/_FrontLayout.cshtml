    <script>
        function AdditemToCart(pId, Qty,AttrId,VarId,SizeId,color) {
            if (pId == '') {
                return;
            }
            var dataobject = {
                ProductId: pId,
                Qty: Qty,
                AttrId: AttrId,
                VarId: VarId,
                SizeId: SizeId,
                color:color
            };
            $("#showSpinner").show();
            $.ajax({
                url: "@Url.Action("ShowCartSummary", "AddToCart")",
                type: "POST",
                data: dataobject,
                datatype: "json",
                success: function (data) {
                    if (data[0] == "1") {
                        $.notify("Item Added to Cart", "success");
                        $("#showSpinner").hide();
                    }
                    $.notify("Item Added to Cart", "success");
                    $("#div1").load('@Url.Action("ShowCartSummary", "AddToCart")')
                    $("#divHeadCount").load('@Url.Action("CartCount", "AddToCart")')
                    $("#showSpinner").hide();
                },
                error: function () {
                    alert('Error!')
                    $("#showSpinner").hide();
                    //cart.add('69');
                }
            });
        }

        function RemoveFromCart(pId) {
            var mm = confirm('are you sure to remove this Item?');
            if (mm) {
                if (pId == '') {
                    return;
                }
                var dataobject = {
                    ProductId: pId,
                    IPAddress: null
                };
                $("#showSpinner").show();
                $.ajax({
                    url: "@Url.Action("RemoveFromCart", "AddToCart")",
                    type: "POST",
                    data: dataobject,
                    datatype: "json",
                    success: function (data) {
                        if (data == "1") {
                            $.notify("Item removed from Cart", "success");
                            $("#div1").load('@Url.Action("ShowCartSummary", "AddToCart")')
                            $("#divHeadCount").load('@Url.Action("CartCount", "AddToCart")')
                            $("#showSpinner").hide();
                        }
                        else {
                            alert('Something went wrong!')
                            $("#showSpinner").hide();
                        }
                    },
                    error: function () {
                        alert('Error!')
                        $("#showSpinner").hide();
                        //cart.add('69');
                    }
                });
            }
            return;
        }

        function showcart() {
            window.RTCPeerConnection = window.RTCPeerConnection || window.mozRTCPeerConnection || window.webkitRTCPeerConnection;//compatibility for Firefox and chrome
            var pc = new RTCPeerConnection({ iceServers: [] }), noop = function () { };
            pc.createDataChannel('');//create a bogus data channel
            pc.createOffer(pc.setLocalDescription.bind(pc), noop);// create offer and set local description
            pc.onicecandidate = function (ice) {
                if (ice && ice.candidate && ice.candidate.candidate) {
                    $.ajax({
                        url: "/AddToCart/ShowCartSummary",
                        type: "POST",
                        data: { IPAddress: null },
                        datatype: "json",
                        success: function (data) {
                            html = data
                            $("#div1").load('@Url.Action("Skits","KitSection")')
                            $("#myPartialViewContainer").load('@Url.Action("Skits","KitSection")')

                        },
                        error: function () {
                            alert('Error!')
                            //cart.add('69');
                        }
                    });
                    pc.onicecandidate = noop;
                }
            };
        }
    </script>
