    function InsertDeliverAddress() {
        debugger;
        if ($("#txtName").val() == "") {
            alert("Please Enter Name !!!");
            $("#txtName").focus();
            return;
        }
        if ($("#txtMobileNo").val() == "") {
            alert("Please Enter Mobile No !!!");
            $("#txtMobileNo").focus();
            return;
        }
        if ($("#txtAddress").val() == "") {
            alert("Please Select Location !!!");
            $("#txtAddress").focus();
            return;
        }


        if ($("#txtAddress2").val() == "") {
            alert("Please Enter Address !!!");
            $("#txtAddress2").focus();
            return;
        }

        if ($("#txtPinCode").val() == "") {
            alert("Please Enter Pin Code !!!");
            $("#txtPinCode").focus();
            return;
        }
        if ($("#txtCityName").val() == "") {
            alert("Please Enter City Name !!!");
            $("#txtCityName").focus();
            return;
        }

        if ($("#ddlState").val() == "") {
            alert("Please Select State !!!");
            $("#ddlState").focus();
            return;
        }
        var AddressType = $("input:radio[name=address1]:checked").val();
        var latitude = "";
        var longitude = "";

        var geocoder = new google.maps.Geocoder();

        geocoder.geocode({ 'address': $("#txtAddress").val() }, function (results, status) {
            if (status == google.maps.GeocoderStatus.OK) {
                latitude = results[0].geometry.location.lat();
                longitude = results[0].geometry.location.lng();
                var dataobject = {
                    SSName: $("#txtName").val(),
                    ContactNo: $("#txtMobileNo").val(),
                    PinCode: $("#txtPinCode").val(),
                    Address: $("#txtAddress").val(),
                    CityName: $("#txtCityName").val(),
                    StCode: $("#ddlState").val(),
                    landmark: $("#txtLandmark").val(),
                    altmobileno: $("#txtAltMobileNo").val(),
                    Address2: $("#txtAddress2").val(),
                    OfferType: AddressType,
                    latitude: latitude,
                    longitude: longitude
                };
                $("#showSpinner").show();
                $.ajax({
                    url: "/Ecommarce/InsertDeliveryAddress",
                    type: "POST",
                    contentType: false,
                    processData: false,
                    //data: dataobject,
                    data: JSON.stringify(dataobject),
                    contentType: "application/json; charset=utf-8",
                    dataType: "json",
                    success: function (r) {
                        if (r.strId == '1') {
                            location.reload();
                        }
                        else {
                            alert('Server not Responding !!!');
                            $("#showSpinner").hide();
                        }
                    },
                    error: function (XMLHttpRequest, textStatus, errorThrown) {
                        alert('Server not Responding !!!');
                        $("#showSpinner").hide();
                    }
                });

            }
            else {
                alert("Please Select Address ");
                return false;
            }
        });

    }

