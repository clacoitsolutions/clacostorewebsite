
@{
    ViewBag.Title = "dontgst";
    Layout = "~/Views/Shared/seller_layout.cshtml";
}



<style>
    .become-seller-section-custom {
        background-color: #f9f9f9;
        padding: 50px 0;
    }

    .container-custom {
        max-width: 1200px;
        margin: 0 auto;
        padding: 0 20px;
    }

    .section-heading-custom {
        text-align: center;
        margin-bottom: 50px;
    }

        .section-heading-custom h2 {
            font-size: 2.5rem;
            color: #333;
        }

    .selling-points-custom {
        display: flex;
        flex-wrap: wrap;
        justify-content: space-between;
        margin-bottom: 50px;
    }

    .point-custom {
        flex: 0 0 calc(50% - 20px);
        background-color: #fff;
        padding: 20px;
        border-radius: 10px;
        box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);
        margin-bottom: 20px;
    }

        .point-custom h3 {
            font-size: 1.5rem;
            color: #333;
        }

        .point-custom p {
            color: #666;
        }

    .join-community-custom,
    .get-started-custom {
        text-align: center;
    }

        .join-community-custom h2,
        .get-started-custom h2 {
            font-size: 2rem;
            margin-bottom: 20px;
        }

    .btn-custom {
        display: inline-block;
        padding: 10px 20px;
        background-color: #007bff;
        color: #fff;
        text-decoration: none;
        border-radius: 5px;
        transition: background-color 0.3s ease;
    }

        .btn-custom:hover {
            background-color: #0056b3;
        }

    .mission-statement-custom {
        background-color: #fff;
        padding: 30px;
        border-radius: 10px;
        box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);
        margin-bottom: 30px;
    }

        .mission-statement-custom h3 {
            font-size: 1.8rem;
            color: #333;
            margin-bottom: 15px;
        }

        .mission-statement-custom p {
            color: #666;
            font-size: 1.2rem;
            line-height: 1.6;
        }

    .services-container-custom {
        display: flex;
        justify-content: center;
        flex-wrap: wrap;
        background-color: white
    }

    .services-card-custom {
        background-color: #fff;
        border-radius: 10px;
        box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);
        margin: 20px;
        padding: 20px;
        width: 340px;
        transition: transform 0.3s ease-in-out;
    }

        .services-card-custom:hover {
            transform: translateY(-5px);
        }

    .services-icon-custom {
        font-size: 48px;
        color: #333;
        text-align: center;
    }

    .services-title-custom {
        color: #333;
        font-size: 24px;
        margin-top: 20px;
        text-align: center;
    }

    .services-text-custom {
        color: #666;
        font-size: 16px;
        margin-top: 20px;
    }
</style>


