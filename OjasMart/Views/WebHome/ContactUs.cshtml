<style>
    /* code by muskan*/
    /* CSS for centering the alert message div */
    #alertMessage {
        position: fixed;
        bottom: 1%;
        left: 50%;
        transform: translate(-50%, -50%);
        height: 50px;
        background: black;
        display: none;
        text-align: center; /* Center the text horizontally */
        color: white;
    }
</style>
<script>
    $(document).ready(function(){
        // Check if TempData["msg"] is not empty
        var message = "@TempData["msg"]";
        if (message.trim() !== "") {
            // Display the alert message
            $("#message").text(message);

            // Check if the message contains "error" or "Error" to display error icon
            if (message.toLowerCase().indexOf("error") !== -1) {
                $("#iconError").show();
                $("#alertMessage");
            } else {
                // Display success icon and set background color to green
                $("#iconSuccess").show();
                $("#alertMessage");
            }

            $("#alertMessage").show();
            setTimeout(function () {
                $("#alertMessage").hide();
            }, 3000);
        }
    });
</script>
